<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="msapplication-tap-highlight" content="no" />
    <!-- Chrome, Firefox OS and Opera -->
    <meta name="theme-color" content="#49CEFF">
    <!-- Windows Phone -->
    <meta name="msapplication-navbutton-color" content="#49CEFF" />
    <!-- iOS Safari -->
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent">
    <title>Test Invoice</title>
    <link rel="stylesheet" href="~/Content/Template/bower_components/bootstrap/dist/css/bootstrap.min.css">

    <link rel="stylesheet" href="~/Content/Template/bower_components/material-design-iconic-font/dist/css/material-design-iconic-font.min.css">
    <link rel="stylesheet" href="~/Content/Template/bower_components/animate.css/animate.min.css">
    <link rel="stylesheet" href="~/Content/Template/bower_components/metisMenu/dist/metisMenu.min.css">
    <link rel="stylesheet" href="~/Content/Template/bower_components/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.css">
    <link rel="stylesheet" href="~/Content/Template/bower_components/Waves/dist/waves.min.css">
    <link rel="stylesheet" href="~/Content/Template/bower_components/toastr/toastr.css">
    <link rel="stylesheet" href="~/Content/Template/bower_components/datatables/media/css/jquery.dataTables.min.css">
    <link rel="stylesheet" href="~/Content/Template/css/style.css">
</head>

<body>
    <!--Preloader-->
    <div id="preloader">
        <div class="refresh-preloader"><div class="preloader"><i>.</i><i>.</i><i>.</i></div></div>
    </div>
    <div class="wrapper">
        <nav class="navbar" style="background:#002A59">
            <div class="navbar-header container">
                <a href="#" class="menu-toggle"><i class="zmdi zmdi-menu"></i></a>
                <a href="@Url.Action("Index","Home")" class="logo">Home</a>
                <a href="@Url.Action("Index","Home")" class="icon-logo"></a>
            </div>
            <div class="navbar-container clearfix">
                <div class="pull-left">
                    <a href="@Url.Action("Index","Home")" class="page-title text-uppercase">Dashboard</a>
                </div>
            </div>
        </nav>
        <aside class="sidebar">
            <ul class="nav metismenu">
                <li class="profile-sidebar-container">
                    <div class="profile-sidebar text-center">
                        <div class="profile-userpic">
                            <img src="~/Content/Template/img/profile_user.jpg" class="img-responsive img-circle center-block" alt="user">
                            <span class="online"></span>
                        </div>
                        <div class="profile-usertitle">
                            <div class="name">
                                Test Invoice
                            </div>
                            <div class="city">
                                <i class="zmdi zmdi-pin"></i>Perfil
                            </div>
                        </div>
                        <div class="profile-activity clearfix">
                            <div class="pull-left">
                                Clientes
                                <br>
                                <span>0</span>
                            </div>
                            <div class="pull-right">
                                Tipo Clientes
                                <br>
                                <span>0</span>
                            </div>
                        </div>
                    </div>
                </li>
                <li><a href="@Url.Action("Index","Home")"><i class="zmdi zmdi-view-dashboard"></i>Inicio</a></li>
                <li>
                    <a href="#"><i class="zmdi zmdi-view-dashboard"></i>Menu<span class="zmdi arrow"></span></a>
                    <ul class="nav nav-inside collapse">
                        <li><a href="@Url.Action("Index","CustomerTypes")">Customer Types</a></li>
                        <li><a href="@Url.Action("Index","Customer")">Clientes</a></li>
                        
                    </ul>

                </li>
               

            </ul>
        </aside>
       
        @RenderBody()
        <footer class="page-footer">© 2016 Copyright</footer>
    </div>

    <script src="~/Content/Template/bower_components/jquery/dist/jquery.min.js"></script>
    <script src="~/Content/Template/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="~/Content/Template/bower_components/metisMenu/dist/metisMenu.min.js"></script>
    <script src="~/Content/Template/bower_components/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.js"></script>
    <script src="~/Content/Template/bower_components/Waves/dist/waves.min.js"></script>
    <script src="~/Content/Template/bower_components/toastr/toastr.js"></script>

    <script src="~/Content/Template/bower_components/datatables/media/js/jquery.dataTables.min.js"></script>
    <script src="~/Content/Template/bower_components/datatables.net-responsive/js/dataTables.responsive.js"></script>
    <script src="~/Content/Template/bower_components/jqvmap/dist/jquery.vmap.min.js"></script>
    <script src="~/Content/Template/bower_components/jqvmap/dist/maps/jquery.vmap.world.js"></script>
    <script src="~/Content/Template/bower_components/jqvmap/examples/js/jquery.vmap.sampledata.js"></script>
    <script src="~/Content/Template/bower_components/moment/min/moment.min.js"></script>

    <script src="~/Content/Template/bower_components/Chart.js/Chart.js"></script>
    <script src="~/Content/Template/bower_components/flot/jquery.flot.js"></script>
    <script src="~/Content/Template/bower_components/flot/jquery.flot.resize.js"></script>
    <script src="~/Content/Template/bower_components/flot.tooltip/js/jquery.flot.tooltip.js"></script>
    <script src="~/Content/Template/bower_components/jquery.easy-pie-chart/dist/jquery.easypiechart.js"></script>

    <script src="~/Content/Template/js/common.js"></script>

    @RenderSection("scripts", required: false)
</body>
</html>
